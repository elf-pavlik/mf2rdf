    rdfs:isDefinedBy <http://microformats.org/terms#> .
    
mf2:p-family-name 
    rdf:type rdf:Property ;
    rdfs:label "p-family-name"@en ;
    owl:sameAs foaf:familyName, vcard:family-name ;
    rdfs:isDefinedBy <http://microformats.org/terms#> .
    
mf2:p-gender-identity
    rdf:type rdf:Property ;
