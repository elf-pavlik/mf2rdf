    rdfs:isDefinedBy <http://microformats.org/terms#> .
    
mf2:p-contact
    rdf:type rdf:Property ;
    rdfs:label "p-contact"@en ;
    rdfs:comment "Current contact info in an h-card."@en ;
    rdfs:isDefinedBy <http://microformats.org/terms#> .
    
mf2:p-count
    rdf:type rdf:Property ;
    rdfs:label "p-count"@en ;
    rdfs:comment "Number of reviews aggregated."@en ;
    rdfs:isDefinedBy <http://microformats.org/terms#> .
    
mf2:p-country-name 
    rdf:type rdf:Property ;
    rdfs:label "p-country-name"@en ;
    owl:sameAs vcard:hasCountryName ;
    rdfs:isDefinedBy <http://microformats.org/terms#> .
    
mf2:p-description 
    rdf:type rdf:Property ;
    rdfs:label "p-description"@en ;
    rdfs:comment "Description of an event."@en ;
    rdfs:isDefinedBy <http://microformats.org/terms#> .
    
mf2:p-education
    rdf:type rdf:Property ;
