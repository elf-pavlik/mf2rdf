<http://microformats.org/wiki/microformats-2#>
  a owl:Ontology ;
  dct:title "Microformats2" ;
  owl:versionInfo "2" ;
  dct:description "Microformats2 is the simplest way to markup structured information in HTML. Microformats2 improves ease of use and implementation for both authors (publishers) and developers (parser implementers)." ;
  dct:created "2015-03-19" ;
  dct:modified "2015-03-19" .

mf2:h-adr
  rdf:type rdfs:Class ;
  rdfs:label "h-adr"@en ;
  rdfs:comment "The h-adr microformat is for marking up structured locations such as addresses, physical and/or postal. This is an update to adr."@en ;
  foaf:isPrimaryTopicOf wiki:h-adr ;
  rdfs:isDefinedBy <http://microformats.org/wiki/microformats-2#> .
  
mf2:h-card
  rdf:type rdfs:Class ;
  rdfs:label "h-card"@en ;
  rdfs:comment "An h-card is a description of a person or an organization."@en ;
  foaf:isPrimaryTopicOf wiki:h-card ;
  rdfs:isDefinedBy <http://microformats.org/wiki/microformats-2#> .

